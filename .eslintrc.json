{
  "extends": ["next/core-web-vitals", "plugin:jsx-a11y/recommended"],
  "plugins": ["simple-import-sort", "jsx-a11y"],
  "rules": {
    "simple-import-sort/imports": "error",
    "complexity": ["error", 3],
    "max-nested-callbacks": ["error", 3],
    "max-depth": ["error", 3],
    "@next/next/no-title-in-document-head": 0
  }
}
